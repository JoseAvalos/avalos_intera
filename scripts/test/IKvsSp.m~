Sp = csvread('S_data.txt');
Ik = csvread('IK_data.txt');
l1=length(Sp);
l2=length(Ik);
t1=0.01*[1:l1];
t2=0.01*[1:l2];
%%
figure
subplot(1,2,1);
legend
for n=1:7   
    plot (t2,Ik(:,n))
    hold on
    legend
end
title('Posicion con Cinematica Inversa')

subplot(1,2,2);
legend
for n=1:7   
    plot (t1,Sp(:,n))
    hold on
    legend
end
title('Posicion con Spline')

%%
plot (t2,Ik(:,4),'r-')
hold on
plot (t1,Sp(:,4))
legend
